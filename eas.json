{
    "cli": {
        "version": ">= 5.2.0",
        "appVersionSource": "local"
  },
    "build": {
       "development": {
       "developmentClient": true,
       "distribution": "internal",
       "env": {
       "EXPO_ENABLE_NEW_ARCHITECTURE": "false"
       },
       "android": {
       "buildType": "apk", 
       "withoutCredentials": true
       },
       "cache": {
       "disabled": true
       }
      },
        "preview": {
        "distribution": "internal",
        "env": {
        "EXPO_ENABLE_NEW_ARCHITECTURE": "false"
        },
        "android": {
        "buildType": "apk", 
        "withoutCredentials": true
        },
        "cache": {
        "disabled": true
        }
      },
        "production": {
        "distribution": "internal",
        "android": {
        "buildType": "apk",
        "withoutCredentials": true
        },
        "cache": {
        "disabled": true
        }
      }
    },
    "submit": {
    "production": {}
    }
  }